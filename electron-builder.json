{
  "productName": "angular-electron",
  "appId": "be.dramco.iwastconfigurator",
  "directories": {
    "output": "release/"
  },
    "files": [
        "**/*",
        "!**/*.ts",
        "!*.code-workspace",
        "!LICENSE.md",
        "!package.json",
        "!package-lock.json",
        "!src/",
        "!e2e/",
        "!hooks/",
        "!angular.json",
        "!_config.yml",
        "!karma.conf.js",
        "!tsconfig.json",
        "!tslint.json",
        "*.js",
        "*.html",
        "release",
        "src/**/*",
        "./build/**/*",
        "./index.html",
        "./src/*.js"
    ],
  "win": {
    "icon": "dist/icon.png",
    "target": [
      "portable"
    ]
  },
  "linux": {
    "icon": "dist/icon.png",
    "target": [
      "AppImage"
    ]
  }
}
